{
	"schemaVersion": 1,
	"id": "compactor",
	"version": "${version}",
	"name": "Compactor",
	"description": "Allows you to compact a few items you can get in bulk from farms.\nAlso gives a few of them some fun functionality",
	"authors": [
		"ImNotRobin"
	],
	"contact": {
		"homepage": "https://modrinth.com/",
		"sources": "https://github.com/ImNotRobin/compactor"
	},
	"license": "CC0-1.0",
	"icon": "assets/compactor/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.robin.compactor.Compactor"
		],
		"client": [
			"net.robin.compactor.ClientCompactor"
		],
		"fabric-datagen": [
			"net.robin.compactor.CompactorDataGenerator"
		]
	},
	"mixins": [
		"compactor.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
